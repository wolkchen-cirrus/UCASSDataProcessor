// Example import specification structure. Leveling is as follows:
// filename -> data -> cols -> column tags, must be in valid config
//                  -> srow -> start row for columnar input
//                  -> procrows -> var name: row number (will import whole row)
//          -> type: Met, FC, &c
{
  "file1.csv": {"data": {"cols": ["dt", "T", "P", "RH"]}, "type": "Met"},
  "file2.csv": {"data": {"cols": ["dt", "C1", "C2", "C3", "C4", "C5", "C6", "C7", "C8", "C9", "C10", "C11", "C12", "C13", "C14", "C15", "C16", "ToF1", "ToF3", "ToF5", "ToF7", "Period", "CSum", "Glitch", "LToF", "RejRat"], "srow":  "5", "procrows": {"bbs":  "3"}}, "type": "UCASS"},
  "file3.log": {"data": {"ARSP": ["Airspeed"], "ATT": ["Roll", "Pitch", "Yaw"], "GPS": ["Lat", "Lng", "Alt", "Spd"], "BARO": ["Press"]}, "type": "FC Proc"}
}